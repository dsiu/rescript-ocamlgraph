{
  "name": "rescript-ocamlgraph",
  "version": "0.0.1",
  "type": "module",
  "engines": {
    "node": ">14.0.0"
  },
  "scripts": {
    "clean": "rescript clean -with-deps",
    "build": "rescript build -with-deps",
    "watch": "rescript build -with-deps -w",
    "start": "rescript build -w"
  },
  "keywords": [
    "ReScript",
    "ocamlgraph"
  ],
  "author": "Danny Siu <danny.siu@gmail.com>",
  "license": "LGPL-2.1-or-later",
  "devDependencies": {
    "@glennsl/rescript-jest": "^0.9.1",
    "esbuild-jest": "^0.5.0",
    "jest": "^28.1.2",
    "rescript": "^10.0.0"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/**/**/*.test.(js|jsx|ts|tsx)",
      "<rootDir>/**/*_Test.bs.js"
    ],
    "transform": {
      "^.+\\.jsx?$": "esbuild-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!(rescript|@glennsl/rescript-jest|@ryyppy/rescript-promise|rationale)/)"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "mjs"
    ]
  },
  "dependencies": {
  }
}
